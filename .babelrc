{
  "presets": [
    ["env", {
      "loose": true,
      "target": {
        "browsers": [
          "> 1%",
          "last 2 versions"
        ]
      }
    }]
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    ["transform-react-jsx", {
      "pragma": "h"
    }]
  ]
}
